package DS.linkedList.Basic.A_Defntn;

/**
 * This is one node of Link-list.
 * Having two fields data and next(This is of type node).
 * 
 * Next takes the reference of "next node" and so on.
 */

import DS.SampleObj.DataObject;

public class Node {
	
	private DataObject data;
	public Node next;
	    	
	public  Node(DataObject data) {		
		 this.data = data;		
	}
	
	public DataObject getData() {
		return data;
	}	
	
}


