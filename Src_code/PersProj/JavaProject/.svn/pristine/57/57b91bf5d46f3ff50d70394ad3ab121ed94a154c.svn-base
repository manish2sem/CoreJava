package DS.String.A_Elementry;

/**
 * char is effectively an unsigned 16-bit integer type in Java.
 *
 */


public class G_InttoChar {

	
	public static void main(String[] args) {
		System.out.println(" Printing character 1 -> " + (char)49 );
		System.out.println(" Printing character 9 -> " + (char)57 );
		System.out.println(" Printing character A -> " + (char)65 );
		System.out.println(" Printing character Z -> " + (char)90 );
		System.out.println(" Printing character a -> " + (char)(97) );
	}

}
