package EffectiveJava.C_ClsIf.Item20.Soln;

import EffectiveJava.C_ClsIf.Item20.Soln.abstrct.Figure;

class Rectangle extends Figure {
	final double length;
	final double width;
	Rectangle(double length, double width) {
		this.length = length;
		this.width = width;
	}
	public double area() { return length * width; }
}
